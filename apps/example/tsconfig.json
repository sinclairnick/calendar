{
  "extends": "tsconfig/base.json",
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "compilerOptions": {
    "jsx": "preserve",
    "strict": true,
    "allowJs": true,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "resolveJsonModule": true
  },
  "exclude": [
    "node_modules"
  ]
}
